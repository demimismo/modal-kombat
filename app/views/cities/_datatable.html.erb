<table id="<%= table_id %>" class="datatable" cellpadding="0" cellspacing="0">
  <thead>
    <tr>
      <th>Año</th>
      <th>A pie</th>
      <th>En bici</th>
      <th>En transporte público</th>
      <th>En coche o moto</th>
    </tr>
  </thead>
  <tbody>
    <% city.datasets.each do |dataset| %>
    <tr>
      <th><%= dataset.year %></th>
      <td class="odd"><%= dataset.pedestrian_share %></td>
      <td class="even"><%= dataset.bike_share %></td>
      <td class="odd"><%= dataset.public_transport_share %></td>
      <td class="even"><%= dataset.motorized_share %></td>
    </tr>
    <% end %>
  </tbody>
</table>
